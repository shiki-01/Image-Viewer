{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": false,
		"jsx": "preserve",
		"lib": ["dom", "es2017"],
		"module": "esnext",
		"moduleResolution": "node",
		"target": "es2017",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"strict": true,
		"outDir": "build",
	},
	"include": ["src/**/*.ts", "src/**/*.js", "src/**/*.svelte"]
}
